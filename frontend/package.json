{
  "name": "clean-calendar",
  "productName": "Clean Calendar",
  "version": "1.0.0",
  "description": "422-proj1-s2018",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . CleanCalendar --overwrite --asar=true --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Clean Calendar\"",
    "package-linux": "electron-packager . CleanCalendar --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icon.png --prune=true --out=release-builds",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewletz/CleanCalendar.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewletz/CleanCalendar/issues"
  },
  "homepage": "https://github.com/andrewletz/CleanCalendar#readme",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-packager": "^12.0.1"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "uuid": "^3.2.1"
  }
}
